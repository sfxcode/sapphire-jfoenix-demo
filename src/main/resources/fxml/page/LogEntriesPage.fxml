<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.sfxcode.sapphire.javafx.control.FXTableValueFactory?>
<AnchorPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sfxcode.sapphire.jfoenix.demo.controller.page.LogEntriesPageController">
    <children>
        <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <top>
                <HBox fx:id="searchBox" styleClass="searchBox" prefHeight="30.0" spacing="10.0" BorderPane.alignment="CENTER">
               <padding>
                  <Insets bottom="10.0" />
               </padding></HBox>
            </top>
            <center>
                <TableView fx:id="table" BorderPane.alignment="CENTER">
                    <columns>
                        <TableColumn text="Event created at">
                            <cellValueFactory>
                                <FXTableValueFactory property="date" format="YYYY-MM-dd HH:mm:ss.SSS"/>
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn text="Topic">
                            <cellValueFactory>
                                <FXTableValueFactory property="topic"/>
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn text="Message">
                            <cellValueFactory>
                                <FXTableValueFactory property="message"/>
                            </cellValueFactory>
                        </TableColumn>
                    </columns>
                </TableView>
            </center>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </BorderPane>
    </children>
</AnchorPane>
